    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#cc3c73",
    "titleBar.inactiveBackground": "#cc3c7399",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#cc3c73",
    "statusBarItem.hoverBackground": "#d76490",
    "statusBar.foreground": "#e7e7e7"
