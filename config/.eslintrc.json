{
  "env": {
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "plugins": ["prettier",  "cypress"],
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": "off",
    "no-console": "off",
    "spaced-comment": "off"
  },
  "globals": {
    "responseMock": "readonly"
  }
}
